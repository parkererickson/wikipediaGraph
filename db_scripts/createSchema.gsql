CREATE VERTEX Article(PRIMARY_ID topic STRING) WITH STATS="OUTDEGREE_BY_EDGETYPE", PRIMARY_ID_AS_ATTRIBUTE="true"
CREATE VERTEX Keyword(PRIMARY_ID keyword STRING) WITH STATS="OUTDEGREE_BY_EDGETYPE", PRIMARY_ID_AS_ATTRIBUTE="true"
CREATE DIRECTED EDGE HAS(FROM Article, TO Keyword) WITH REVERSE_EDGE="reverse_HAS"
CREATE DIRECTED EDGE LINKED_TO(FROM Article, TO Article) WITH REVERSE_EDGE="reverse_LINKED_TO"

CREATE GRAPH @graphname@(Article, Keyword, HAS, LINKED_TO)