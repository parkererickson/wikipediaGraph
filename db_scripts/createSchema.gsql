CREATE VERTEX Question(PRIMARY_ID questionID STRING, fold STRING COMPRESS, text STRING COMPRESS) WITH STATS="OUTDEGREE_BY_EDGETYPE", PRIMARY_ID_AS_ATTRIBUTE="true"
CREATE VERTEX Category(PRIMARY_ID categoryName STRING) WITH STATS="OUTDEGREE_BY_EDGETYPE", PRIMARY_ID_AS_ATTRIBUTE="true"
CREATE VERTEX Answer(PRIMARY_ID title STRING) WITH STATS="OUTDEGREE_BY_EDGETYPE", PRIMARY_ID_AS_ATTRIBUTE="true"
CREATE VERTEX Keyword(PRIMARY_ID word STRING) WITH STATS="OUTDEGREE_BY_EDGETYPE", PRIMARY_ID_AS_ATTRIBUTE="true"
CREATE DIRECTED EDGE QUESTION_IS_IN_CATEGORY(FROM Question, TO Category, weight DOUBLE DEFAULT "1.0") WITH REVERSE_EDGE="reverse_QUESTION_IS_IN_CATEGORY"
CREATE DIRECTED EDGE ANSWERS(FROM Answer, TO Question, weight DOUBLE DEFAULT "1.0") WITH REVERSE_EDGE="reverse_ANSWERS"
CREATE DIRECTED EDGE QUESTION_CONTAINS(FROM Question, TO Keyword, weight DOUBLE DEFAULT "1.0") WITH REVERSE_EDGE="reverse_QUESTION_CONTAINS"
CREATE DIRECTED EDGE ANSWER_CONTAINS(FROM Answer, TO Keyword, weight DOUBLE DEFAULT "1.0") WITH REVERSE_EDGE="reverse_ANSWER_CONTAINS"

CREATE GRAPH @graphname@(Question, Category, Answer, Keyword, QUESTION_IS_IN_CATEGORY, ANSWERS, QUESTION_CONTAINS, ANSWER_CONTAINS)